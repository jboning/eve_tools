@import 'nib'

@import url(http://fonts.googleapis.com/css?family=Anton);

body
	padding: 50px
	font: 14px "Lucida Grande", Helvetica, Arial, sans-serif

.navbar-inner
	background-color: #345566

.nav.navbar-nav > li > a
	color: white
	&:hover {
	 	color:white
	 	background:#999999
	}

.navbar-inverse .navbar-brand
	font-family: 'Anton', sans-serif
	color: #ABCDEF
	&:hover {
		color: white
	}

.foo-old
	color:white
	font: @font-family-sans-serif
	padding: 10px 19px
	border-radius: 5px
	background: #abcdef
.foo
	background: linear-gradient(top, #abcdef, #6abcde)
